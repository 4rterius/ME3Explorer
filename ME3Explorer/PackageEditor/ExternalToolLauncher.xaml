<me3Explorer:ExportLoaderControl x:Class="ME3Explorer.PackageEditor.ExternalToolLauncher"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ME3Explorer.PackageEditor"
             xmlns:me3Explorer="clr-namespace:ME3Explorer"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid Margin="5">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="300"/>
            <ColumnDefinition Width="300"/>
        </Grid.ColumnDefinitions>
        <StackPanel Grid.Column="0">

        </StackPanel>
        <StackPanel Grid.Column="1">
            <TextBlock Text="Scaleform/Adobe Flash" FontWeight="Bold"/>
            <Button Content="Open with JPEXs" FontWeight="Bold" HorizontalAlignment="Left" Click="OpenWithJPEX_Click"/>
        </StackPanel>
    </Grid>
</me3Explorer:ExportLoaderControl>
